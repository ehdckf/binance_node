{
  "env": {
      "browser": true,
      "node": true,
      "commonjs": true,
      "es2021": true
  },
  "extends": ["eslint:recommended","plugin:prettier/recommended"],
  "overrides": [
  ],
  "parserOptions": {
      "ecmaVersion": "latest"
  },
  "rules": {
      "linebreak-style": 0,
      "indent":["error",8],
      "max-len": [1, { "code": 200, "tabWidth":8 ,"ignoreComments": true,"ignoreTrailingComments": true ,"ignoreUrls": true,"ignoreStrings": true ,"ignoreTemplateLiterals": true,"ignoreRegExpLiterals": true }],
      "prettier/prettier": ["error", { "endOfLine": "auto", "tabWidth":8 ,"print-width":999}]
  }
}
